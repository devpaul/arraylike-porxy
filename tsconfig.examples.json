{
	"version": "2.0.0",
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"baseUrl": ".",
		"declaration": true,
		"module": "AMD",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"outDir": "./_build/examples",
		"preserveConstEnums": true,
		"removeComments": true,
		"sourceMap": true,
		"target": "ES6",
		"paths": {
			"src/*": [
				"./_build/src/*"
			]
		}
	},
	"files": [
	],
	"include": [
		"./examples/**/*.ts"
	]
}
